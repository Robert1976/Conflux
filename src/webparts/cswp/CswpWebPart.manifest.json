    "properties": {
      "description": "CSWP Webpart",
      "query": "document",
      "itemTemplate": "<div>_# Title #_</div>",
      "controlTemplate":  "<div><h3 class=\"test ms-fontSize-xl ms-fontWeight-light\">Test</h3><div>{items}</div></div>",
      "cssStyles": ".test {color: \"[theme:themePrimary, default: blue]\"}",
      "maxNumberResults": 3,
      "noResultsTemplate": "<div class=\"test\">Sorry, no results to display...</div>"
    }
  }]
